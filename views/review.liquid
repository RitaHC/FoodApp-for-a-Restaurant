{% layout 'layout.liquid'%}
{% block content %}
<div>
    <h2 class="display-6">Food Menu</h2>
    <div class="row">
        {% for m in menu %}
        <div style="width: 18rem;">
            <img src={{ m.img }}>
            <h5> {{m.name}} </h5>
        </div>
        {% endfor %}
    </div>
</div>
{% endblock %}